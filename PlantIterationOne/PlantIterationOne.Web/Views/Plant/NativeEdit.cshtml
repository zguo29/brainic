
@model PlantIterationOne.Web.Model.Native
@{
    ViewBag.Title = "编辑植物";
}

    <form asp-action="NativeEdit" asp-controller="Plant" method="POST" enctype="multipart/form-data">
        <input type="hidden" asp-for="Id" />
        <div class="form-group row">
            <label asp-for="Name" class="col-sm-2"></label>
            <div class="col-sm-10">
                <input class="form-control" asp-for="Name">
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="ImageUrl" class="col-sm-2">Upload Image</label>
            <div class="col-sm-10">
                <input class="form-control" asp-for="ImageUrl">
            </div>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
        <a asp-action="NativeList" class="btn btn-secondary">Back To List</a>
    </form>